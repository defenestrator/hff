{
    "name": "defenestrator/hff",
    "description": "Hobo Fly Fishing",
    "keywords": [
        "fly fishing",
        "travel",
        "awesome"
    ],
    "license": "Copyright 2019 Hobo Fly Fishing, LLC. All Rights Reserved, No License for Re-Use is granted.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "aws/aws-php-sns-message-validator": "^1.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "cviebrock/eloquent-taggable": "^3.2",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "greggilbert/recaptcha": "^2.2",
        "illuminate/pagination": "^5.5",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^5.0",
        "laravel/spark": "*@dev",
        "laravel/tinker": "~1.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mpociot/vat-calculator": "^2.2",
        "mtdowling/cron-expression": "^1.2",
        "orchestra/parser": "^3.5",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "spatie/laravel-backup": "^5.1",
        "spatie/laravel-responsecache": "^4.0",
        "spatie/laravel-sitemap": "^3.3",
        "spatie/schema-org": "^1.4",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "defenestrator/test-printer": "^3.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "./spark"
        }
    ]
}
