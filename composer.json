{
    "name": "defenestrator/hff",
    "description": "Hobo Fly Fishing",
    "keywords": [
        "fly fishing",
        "travel",
        "awesome"
    ],
    "license": "Copyright 2017 Hobo Fly Fishing, LLC. All Rights Reserved, No License for Re-Use is granted.",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aws/aws-php-sns-message-validator": "^1.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "cmgmyr/messenger": "^2.14",
        "cviebrock/eloquent-taggable": "^3.2",
        "fideloper/proxy": "~3.3",
        "greggilbert/recaptcha": "^2.2",
        "illuminate/pagination": "^5.5",
        "intervention/imagecache": "^2.3",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/spark": "*@dev",
        "laravel/tinker": "~1.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "mtdowling/cron-expression": "^1.2",
        "php-ffmpeg/extras": "^0.3.1",
        "php-ffmpeg/php-ffmpeg": "^0.9.5",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "spatie/laravel-medialibrary": "^6.1",
        "spatie/laravel-responsecache": "^4.0",
        "yajra/laravel-disqus": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "./spark"
        }
    ]
}
